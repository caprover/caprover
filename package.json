        "dev": "npm run build && sudo ./dev-scripts/dev-reset-service.sh",
        "clean": "npm run build && sudo ./dev-scripts/dev-clean-run-as-dev.sh",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "formatter": "prettier --check './src/**/*.ts'",
